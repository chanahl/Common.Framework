<?xml version="1.0"?>
<package >
  <metadata>
    <!--
      See: https://docs.microsoft.com/en-us/nuget/schema/nuspec
      <Metadata>                  Token           Value source  Value
      
      [Required]
      <id>                        $id$            Project file 	AssemblyName from the project file
      <version>                   $version$       AssemblyInfo 	AssemblyInformationalVersion if present, otherwise AssemblyVersion
      <description>               $description$   AssemblyInfo 	AssemblyDescription
      <authors>                   $author$        AssemblyInfo 	AssemblyCompany
      
      [Optional]
      <title>                     $title$         Project file  AssemblyName from the project file
      <owners>                    n/a             *             A comma-separated list of the package creators. This is often the same list as in authors.
      <projectUrl>                n/a             *             A URL for the package's home page, often shown in UI displays as well as nuget.org.
      <licenseUrl>                n/a             *             A URL for the package's license, often shown in UI displays as well as nuget.org.
      <iconUrl>                   n/a             *             A URL for a 64x64 image with transparency background to use as the icon for the package in UI display.
      <requireLicenseAcceptance>  n/a             *             A Boolean value specifying whether the client must prompt the consumer to accept the package license before installing the package.
      <releaseNotes>              n/a             *             (1.5+) A description of the changes made in this release of the package, often used in UI like the Updates tab of the Visual Studio Package Manager in place of the package description.
      <copyright>                 $copyright$     AssemblyInfo 	AssemblyCopyright
      <tags>                      n/a             *             A space-delimited list of tags and keywords that describe the package and aid discoverability of packages through search and filtering mechanisms.
    -->
    <id>$id$</id>
    <version>$version$</version>
    <description>$description$</description>
    <authors>$author$</authors>
    <title>$title$-$version$</title>
    <owners>$author$</owners>
    <projectUrl>https://github.com/chanahl/.NET</projectUrl>
    <licenseUrl>https://github.com/chanahl/.NET/blob/master/LICENSE</licenseUrl>
    <iconUrl>file://favicon.ico</iconUrl>
    <requireLicenseAcceptance>false</requireLicenseAcceptance>
    <releaseNotes>None</releaseNotes>
    <copyright>$copyright$</copyright>
    <tags>None</tags>
  </metadata>
</package>